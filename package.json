{
  "name": "vpn-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": [
    "src/index.css",
    "src/styles/**",
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "build:inline": "npx tsx build-inline.ts",
    "detect:hex": "node scripts/detect-raw-hex.js",
    "preview": "vite preview --host --port 80",
    "analyze": "node scripts/code-health.js plan-optimize",
    "analyze:unused": "node scripts/code-health.js analyze-unused",
    "analyze:bundle": "npm run build && npx vite-bundle-analyzer dist/assets/*.js",
    "build:analyze": "npm run build && npm run analyze",
    "clean:unused": "node scripts/code-health.js clean-unused",
    "clean:dry-run": "node scripts/code-health.js clean-unused",
    "cleanup:deps": "node scripts/code-health.js cleanup-deps",
    "cleanup:files": "node scripts/code-health.js clean-unused",
    "cleanup:files:safe": "node scripts/code-health.js clean-unused",
    "cleanup:all": "node scripts/code-health.js cleanup-all",
    "audit:code": "npx unimported && npx depcheck",
    "optimize:advanced": "node scripts/code-health.js optimize-advanced",
    "report:final": "node scripts/code-health.js report-final",
    "analyze:heavy": "node scripts/code-health.js analyze-heavy",
    "analyze:complete": "node scripts/code-health.js analyze-complete",
    "plan:optimize": "node scripts/code-health.js plan-optimize",
    "optimize:phase1": "node scripts/code-health.js optimize-phase1"
  },
  "dependencies": {
    "lucide-react": "0.525.0",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-simple-maps": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "24.0.10",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/react-simple-maps": "^3.0.6",
    "@vitejs/plugin-react": "4.6.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.48.0",
    "unimported": "^1.31.0",
    "vite": "7.0.2",
    "vite-bundle-analyzer": "^1.2.3"
  }
}
