{
  "entry": [
    "src/main.tsx",
    "src/App.tsx"
  ],
  "extensions": [
    ".ts",
    ".tsx",
    ".js",
    ".jsx"
  ],
  "ignorePatterns": [
    "**/node_modules/**",
    "**/dist/**",
    "**/*.test.*",
    "**/*.spec.*",
    "**/_archive_unused/**"
  ],
  "ignoreUnresolved": [
    "react",
    "react-dom",
    "vite"
  ],
  "respectGitignore": true
}